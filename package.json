{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx force-resolutions",
    "postinstall": "pnpm setup",
    "prepare": "husky install",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:frontend\"",
    "dev:server": "nodemon --watch server --ext ts,tsx --exec tsx server/index.ts",
    "dev:frontend": "vite --config vite.config.ts",
    "build": "turbo run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "check": "node scripts/check-all.mjs",
    "check-all": "node scripts/check-all.mjs",
    "format-all": "node scripts/format-all.mjs",
    "typecheck": "tsc -b",
    "test": "turbo run test && vitest run",
    "stylelint": "stylelint '**/*.{css,scss}'",
    "db:push": "drizzle-kit push",
    "setup": "node scripts/setup.mjs",
    "pre-commit": "node .husky/pre-commit",
    "pre-push": "node .husky/pre-push",
    "fix-peers": "zx scripts/resolve-peer-conflicts.mjs"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "resolutions": {
    "esbuild": "^0.25.4",
    "@codingame/monaco-vscode-0c06bfba-d24d-5c4d-90cd-b40cefb7f811-common": "17.1.1",
